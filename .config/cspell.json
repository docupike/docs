{
    "language": "en_US",
    "minWordLength": 3,
    "dictionaries": [
        "en_US",
        "project"
    ],
    "dictionaryDefinitions": [
        {
            "name": "project",
            "path": "./spelling.txt"
        }
    ],
    "ignoreRegExpList": [
        "/(~~~+)\\s?[\\s\\S]+?\\1/g",
        "/(```+)\\s?[\\s\\S]+?\\1/g",
        "/`(.+)`/g",
        "/{{ (.+) }}/g",
        "/{% (.+) %}/g"
    ],
    "ignorePaths": [
        "**/changelog.md"
    ]
}
